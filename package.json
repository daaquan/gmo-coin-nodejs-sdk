{
  "name": "gmo-coin-sdk",
  "version": "1.1.0",
  "description": "GMO Coin Private API SDK for Forex and Cryptocurrency (Node.js/TypeScript)",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "examples:rest": "npm run build && node dist/examples/private-rest.js",
    "examples:ws": "npm run build && node dist/examples/private-ws.js",
    "start:service": "npm run build && node dist/service/server.js",
    "lint": "eslint 'src/**/*.ts' 'service/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'service/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts' '*.js' '*.json' '*.md' '*.yml'"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "ioredis": "^5.5.0",
    "jose": "^5.10.0",
    "pino": "^9.6.0",
    "prom-client": "^15.1.3",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitest/coverage-v8": "^3.0.4",
    "@vitest/ui": "^3.0.4",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  }
}