{
  "name": "gmo-coin-sdk",
  "version": "1.1.0",
  "description": "GMO Coin Private API SDK for Forex and Cryptocurrency (Node.js/TypeScript)",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "examples:rest": "npm run build && node dist/examples/private-rest.js",
    "examples:ws": "npm run build && node dist/examples/private-ws.js",
    "start:service": "npm run build && node dist/service/server.js",
    "lint": "eslint 'src/**/*.ts' 'service/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts'"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "jose": "^5.8.0",
    "pino": "^9.4.0",
    "prom-client": "^15.1.3",
    "ws": "^8.17.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.36.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  }
}
